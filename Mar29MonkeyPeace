class Mar29MonkeyPeace {
	int monkeypeace(int n){
		for (int i=0; true; ++i) {
			int temp = i;
			for(int j =1; temp % 5 == 1; ++j){
				temp = (temp - 1) * 4 / 5;
				if ( j == n) {
					return i;
				}
			}
		}
	}

	public static void main(String[] args) {
		Mar29MonkeyPeace monkeyPeace = new Mar29MonkeyPeace();
		System.out.println(monkeyPeace.monkeypeace(5));
	}
}
